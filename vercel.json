{
    "version": 2,
    "builds": [
      {"src": "api.py", "use": "@vercel/python"}
    ],
    "routes": [
      {"src": "/(.*)", "dest": "api.py",
    "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"],
      "headers": {
        "Access-Control-Allow-Origin": "*"
      }}
    ]
  }
  